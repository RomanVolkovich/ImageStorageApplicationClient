{
  "version": 1,
  "projects": {
    "imageStorageClient": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/imageStorageClient",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.json",
            "aot": true,
            "styles": [
              "src/styles.css"
            ],
            "assets": [
              "src/assets"
            ]
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "imageStorageClient:build"
          }
        }
      }
    }
  },
  "defaultProject": "imageStorageClient",
  "cli": {
    "analytics": false
  }
}
